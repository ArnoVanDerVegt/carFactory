#include "factoryData.whl"

object Car
    number position
    number seat
    number seatReady
    number body
    number bodyReady
end

proc Car.init()
    position = 0
end

proc Car.setOrder(number seat, number body)
    seatReady = FALSE
    bodyReady = FALSE
    self.seat = seat
    self.body = body
    printS(seatColors[seat])
    printS(bodyColors[body])
end
