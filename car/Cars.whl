#include "inventory/Order.whl"
#include "car/Car.whl"

object Cars
    Car  car0
    Car  car1
    Car  car2
    Car  car3
    ^Car cars[4]
end

proc Cars.init()
    cars[0] = @car0
    cars[1] = @car1
    cars[2] = @car2
    cars[3] = @car3
end

proc Cars.order(Order ^order)
    number n
    for n = 0 to 3
        cars[n].setOrder(order.orderLine[n].seat, order.orderLine[n].body)
    end
end
